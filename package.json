{
  "name": "powerdey",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx run-many --target serve --projects app,api",
    "build": "nx build",
    "build:api": "nx run api:build --skip-nx-cache",
    "build:local": "nx run-many --target build --all",
    "build:production": "nx run-many --target build --all",
    "build:watch": "yarn nx run-many --target build --all --watch --configuration development",
    "test": "nx run-many --target test --all",
    "lint": "nx run-many --target lint --all",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:format": "nx affected --target format",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write && nx run-many --target format --all",
    "format:write": "nx format:write && nx run-many --target format --all",
    "format:check": "nx format:check",
    "lint:fix": "nx run-many --target lint --all --fix",
    "update": "nx migrate latest && nx migrate --run-migrations && rm migrations.json",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "test:ui:watch": "jest --watch",
    "deploy:functions:staging": "firebase deploy --only functions,firestore:rules --message \"$(git log -1 --pretty=%s)\"",
    "setup": "husky install",
    "emulate": "firebase emulators:start --export-on-exit=./local --import=./local",
    "serve": "nx run hosting:serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.1.1",
    "@angular/cdk": "15.1.1",
