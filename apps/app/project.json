{
  "name": "app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/app/src",
  "prefix": "powerdey",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/app",
        "index": "apps/app/src/index.html",
        "main": "apps/app/src/main.ts",
        "polyfills": "apps/app/src/polyfills.ts",
        "tsConfig": "apps/app/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/app/src/favicon.ico", "apps/app/src/assets"],
        "styles": [
          "@angular/material/prebuilt-themes/indigo-pink.css",
          "apps/app/src/styles.scss"
        ],
        "scripts": [],
        "i18nMissingTranslation": "error",
        "localize": ["cpe-NG"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "625kb",
              "maximumError": "1.25mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/app/src/environments/environment.ts",
              "with": "apps/app/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "localize": ["cpe-NG", "en-US"]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        },
        "en-US": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "baseHref": "/en-US",
          "deployUrl": "/en-US",
          "localize": ["en-US"]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "proxyConfig": "apps/app/proxy.conf.json"
      },
      "configurations": {
        "production": {
          "browserTarget": "app:build:production"
        },
        "development": {
          "browserTarget": "app:build:development",
          "port": 4200
        },
        "en-US": {
          "browserTarget": "app:build:development,en-US",
          "port": 4201,
          "proxyConfig": "apps/app/proxy.conf.en.json",
          "servePath": "/en-US"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "app:build",
        "outputPath": "apps/app/src/locale"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/app/**/*.ts", "apps/app/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/app"],
      "options": {
        "jestConfig": "apps/app/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "serve-i18n": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx run app:serve:development", "nx run app:serve:en-US"]
      }
    }
  },
  "i18n": {
    "sourceLocale": "cpe-NG",
    "locales": {
      "en-US": "apps/app/src/locale/messages.en.xlf"
    }
  },
  "tags": []
}
